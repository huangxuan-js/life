














/*每个页面公共css */
.content {
	padding: 15px;
}
.content input {
	height: 46px;
	border: solid 1px #DDDDDD;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
	margin-bottom: 15px;
	padding: 0px 15px;
}
.content button {
	margin-bottom: 15px;
}
.content navigator {
	display: inline-block;
	color: #007AFF;
	border-bottom: solid 1px #007AFF;
	font-size: 16px;
	line-height: 24px;
	margin-bottom: 15px;
}
.content .row {
	margin-bottom: 15px;
}
.tips {
	text-align: center;
	line-height: 20px;
	font-size: 14px;
	color: #999999;
	margin-bottom: 20px;
}

