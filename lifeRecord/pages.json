{
	"easycom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
		    "path" : "pages/my/my",
		    "style" : {}
		},
		{
		    "path" : "pages/login/login",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "登入",
		        "enablePullDownRefresh": false
		    }
		}
		,{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": ""
			}
		}
        ,{
            "path" : "pages/journal/journal",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/bookkeeping/bookkeeping",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"tabBar": {
	    "color": "#707070", //文字默认颜色
	    "selectedColor": "#3286fd", //选择文字颜色
	    "borderStyle": "black", //tabBar 边框颜色 可选black/white
	    "backgroundColor": "#ffffff", //tabBar 背景色
	    "list": [{
	        "pagePath": "pages/home/home", //页面路径 
	        "iconPath": "static/images/home.png", //默认图片
	        "selectedIconPath": "static/images/home_active.png", //选中展示图片
	        "text": "首页"
	    }, {
	        "pagePath": "pages/bookkeeping/bookkeeping",
	        "iconPath": "static/images/bookkeeping.png",
	        "selectedIconPath": "static/images/bookkeeping_active.png",
	        "text": "记账"
	    }, {
	        "pagePath": "pages/journal/journal",
	        "iconPath": "static/images/journal.png",
	        "selectedIconPath": "static/images/journal_active.png",
	        "text": "日志"
	    }, {
	        "pagePath": "pages/my/my",
	        "iconPath": "static/images/my.png",
	        "selectedIconPath": "static/images/my_active.png",
	        "text": "我的"
	    }]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
